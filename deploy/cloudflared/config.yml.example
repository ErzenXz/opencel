tunnel: opencel
credentials-file: /etc/cloudflared/opencel.json

ingress:
  - hostname: opencel.example.com
    service: http://localhost:80
  - hostname: "*.preview.opencel.example.com"
    service: http://localhost:80
  - hostname: "*.prod.opencel.example.com"
    service: http://localhost:80
  - service: http_status:404

